server {
    listen 8002;
    server_name localhost;
    host 127.0.0.1;
    root ;
    client_max_body_size 3000000;
    index /www/index/index.html;
    error_page 404 /www/error_pages/404.html;

    location / {
        allow_methods  DELETE POST GET;
        autoindex off;
    }
}

